<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.DzmPatientMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.example.demo.pojo.DzmPatient" id="dzmPatientMap">
        <result property="patientId" column="patient_id"/>
        <result property="name" column="name"/>
        <result property="mobile" column="mobile"/>
        <result property="updateTime" column="update_time"/>
        <result property="sex" column="sex"/>
        <result property="birthday" column="birthday"/>
        <result property="idCard" column="id_card"/>
        <result property="miCard" column="mi_card"/>
        <result property="address" column="address"/>
        <result property="createTime" column="create_time"/>
        <result property="allergyInfo" column="allergy_info"/>
        <result property="isDel" column="is_del"/>
    </resultMap>

</mapper>